{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "userId": {
        "type": "integer",
        "description": "The ID of the user associated with the entry"
      },
      "id": {
        "type": "integer",
        "description": "The unique identifier for the entry"
      },
      "title": {
        "type": "string",
        "description": "The title of the entry"
      },
      "body": {
        "type": "string",
        "description": "The body text or content of the entry"
      }
    },
    "required": ["userId", "id", "title", "body"],
    "additionalProperties": false
  }
}